finverser <- function(vec_col){
unique_col <- unique(vec_col)
unique_col <- unique_col[!is.na(unique_col)]
n <- length(unique_col)
max <- max(vec_col, na.rm = T)
ord <- sort(as.vector(unique_col))
rev <- rev(ord)
for (i in 1:n){
vec_col[vec_col == ord[i]] <- max + rev[i]
}
vec_col <- vec_col - max
return(vec_col)
}
